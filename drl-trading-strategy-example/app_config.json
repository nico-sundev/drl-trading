{
  "localDataImportConfig": {
    "symbols": [
      {
        "symbol": "MYSYMBOL",
        "datasets": [
          {
            "timeframe": "H1",
            "base_dataset": true,
            "file_path": "path/to/your/data_H1.csv"
          },
          {
            "timeframe": "D1",
            "base_dataset": false,
            "file_path": "path/to/your/data_D1.csv"
          }
        ]
      }
    ],
    "limit": 100
  },
  "featuresConfig": {
    "featureDefinitions": [
      {
        "name": "rsi",
        "enabled": true,
        "derivatives": [1],
        "parameterSets": [
          {
            "enabled": true,
            "length": 14
          }
        ]
      }
    ]
  },
  "rlModelConfig": {
    "agents": ["PPO"],
    "trainingSplitRatio": 0.8,
    "validatingSplitRatio": 0.1,
    "testingSplitRatio": 0.1,
    "agent_threshold": 0.1,
    "total_timesteps": 5000
  },
  "environmentConfig": {
    "fee": 0.001,
    "slippageAtrBased": 0.01,
    "slippageAgainstTradeProbability": 0.5,
    "startBalance": 10000.0,
    "maxDailyDrawdown": 0.03,
    "maxAlltimeDrawdown": 0.1,
    "maxPercentageOpenPosition": 50.0,
    "minPercentageOpenPosition": 1.0,
    "inMoneyFactor": 1.0,
    "outOfMoneyFactor": 1.0,
    "liquidationPenaltyFactor": 2.0,
    "minLiquidationPenalty": 50.0,
    "maxTimeInTrade": 20,
    "optimalExitTime": 5,
    "variancePenaltyWeight": 0.5,
    "atrPenaltyWeight": 0.3
  },
  "featureStoreConfig": {
    "enabled": false,
    "configDirectory": "path/to/your/feature_repo",
    "entityName": "symbol",
    "ttlDays": 1,
    "onlineEnabled": false,
    "serviceName": "ai_trading_feature_definitions",
    "serviceVersion": "0.0.1",
    "offlineRepoStrategy": "local",
    "localRepoConfig": {
      "repoPath": "path/to/your/feature_store_data"
    },
    "s3RepoConfig": {
      "bucketName": "drl-trading-features",
      "prefix": "features",
      "endpointUrl": null,
      "region": "us-east-1",
      "accessKeyId": null,
      "secretAccessKey": null
    }
  },
  "contextFeatureConfig": {
    "primaryContextColumns": ["High", "Low", "Close"],
    "derivedContextColumns": ["Open", "Volume"],
    "optionalContextColumns": ["Atr"],
    "timeColumn": "Time"
  }
}
